<script>
  var circleOpt = {
    color      : '#666',
    trailColor : '#ccc',
    strokeWidth: 3,
    trailWidth : 3
  };
  function circle(id, per) {
    (new ProgressBar.Circle(id, circleOpt)).animate(per)
  }

  circle('#webstorm', .85);
  circle('#sublime', .75);
  circle('#atom', .7);
