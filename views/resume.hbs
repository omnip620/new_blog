    #main {
      border-left: 2px solid #ccc;
      padding-left: 20px;
      padding-bottom: 20px;
    }

    #main p {
      margin-bottom: 12px;
    }

    #main p, #main li {
      line-height: 1.6em;
    }

    .title {
      position: relative;
      margin: 20px 0;
    }

    .title span {
      position: relative;
      z-index: 2;
      background-color: #FBFBFB;
      padding-right: 18px;
    }

    h3.title:after {
      display: block;
      position: absolute;
      content: ' ';
      width: 100%;
      height: 2px;
      background-color: #CCCCCC;
      top: 10px;
      z-index: 1;
    }

    @media print {
      .title span {
        position: relative;
        z-index: 2;
        background-color: #fff;
        padding-right: 18px;
      }
    }

    #contact li {
      margin: 14px 0;
    }

    .progress {
      background-color: #ccc;
      height: 12px;
    }

    .progress-bar {
      background-color: #666;
      height: 100%;
      line-height: 12px;
    }

    .circle {
      position: relative;
      width: 100px;
      height: 100px;
      line-height: 100px;
      float: left;
      margin: 10px 15px;
      text-align: center;
    }

    .circle svg {
      position: absolute;
      top: 0;
      left: 0;
    }

    .skill-item {
      margin: 20px 0;
