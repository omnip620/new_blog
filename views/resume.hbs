  <style type="text/css" media="screen,print">
    * {
      box-sizing: border-box;
      margin: 0;
      padding: 0;
    }

    @media print {
      a:visited {
        color: #555;
      }

      a {
        text-decoration: none;
      }
    }

    body {
      color: #555;
      font-family: Helvetica, Tahoma, Arial, 'Microsoft YaHei', 微软雅黑, STXihei, 华文细黑, SimSun, 宋体, Heiti, 黑体, sans-serif;
      font-size: 16px;
      background-color: #fbfbfb;
      -webkit-font-smoothing: antialiased;
      -webkit-print-color-adjust: exact;
    }

    ul {
      margin: 0;
      padding: 0;
      list-style: none;
    }

    [class^="icon-"], [class*=" icon-"] {
      width: 26px;
      display: inline-block;
      font-size: 18px;
      position: relative;
      top: 2px;
      padding-right: 8px;
    }

    .pull-right {
      float: right;
    }

    .clearfix:before {
      content: " ";
      display: table
    }

    .clearfix:after {
      content: " ";
      display: table
    }

    #content {
      width: 960px;
      margin: 0 auto;
    }

    #basic-info, #more-info {
      float: left;
    }

    #basic-info {
      width: 260px;
      position: fixed;
    }

    #more-info {
      padding-left: 285px;
      width: 100%;
    }

    #main {
      border-left: 2px solid #ccc;
      padding-left: 20px;
      padding-bottom: 20px;
    }

    #main p {
      margin-bottom: 12px;
    }

    #main p, #main li {
      line-height: 1.6em;
    }

    .title {
      position: relative;
      margin: 20px 0;
    }

    .title span {
      position: relative;
      z-index: 2;
      background-color: #FBFBFB;
      padding-right: 18px;
    }

    h3.title:after {
      display: block;
      position: absolute;
      content: ' ';
      width: 100%;
      height: 2px;
      background-color: #CCCCCC;
      top: 10px;
      z-index: 1;
    }

    @media print {
      .title span {
        position: relative;
        z-index: 2;
        background-color: #fff;
        padding-right: 18px;
      }
    }

    #contact li {
      margin: 14px 0;
    }

    .progress {
      background-color: #ccc;
      height: 12px;
    }

    .progress-bar {
      background-color: #666;
      height: 100%;
      line-height: 12px;
    }

    .circle {
      position: relative;
      width: 100px;
      height: 100px;
      line-height: 100px;
      float: left;
      margin: 10px 15px;
      text-align: center;
    }

    .circle svg {
      position: absolute;
      top: 0;
      left: 0;
    }

    .skill-item {
      margin: 20px 0;
    }

    .skill-item span {
      float: left;
    }

    .skill-item .progress {
      width: 175px;
      margin-left: 85px;
    }

    #name {
      text-align: right;
    }

    #name > h1 {
      font-size: 45px;
    }

    #name > small {
      font-size: 16px;
    }

    .work > .title {
      margin-bottom: 2px;
    }

    .company {
      font-size: 14px;
      font-style: italic;
      color: #999;
    }

    .work ul {
      padding-left: 20px;
      list-style: disc;
    }

    .work li {
      padding: 4px 0;
    }
  </style>
</head>
