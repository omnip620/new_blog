    ul {
      margin: 0;
      padding: 0;
      list-style: none;
    }

    [class^="icon-"], [class*=" icon-"] {
      width: 26px;
      display: inline-block;
      font-size: 18px;
      position: relative;
      top: 2px;
      padding-right: 8px;
    }

    .pull-right {
      float: right;
    }

    .clearfix:before {
      content: " ";
      display: table
    }

    .clearfix:after {
