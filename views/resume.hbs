    }

    .work > .title {
      margin-bottom: 2px;
    }

    .company {
      font-size: 14px;
      font-style: italic;
      color: #999;
    }

    .work ul {
      padding-left: 20px;
      list-style: disc;
    }

    .work li {
      padding: 4px 0;
    }
  </style>
</head>
